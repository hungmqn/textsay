{
  "extends": [
    "eslint:recommended"
  ],
  "globals": {
    "global": false,
    "__dirname": false,
    "require": false,
    "config": false,
    "env": false,
    "module": false,
    "process": false
  },
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi-style": 2,
    "no-unreachable": 2,
    "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
    "no-else-return": "off",
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": 2,
    "no-eval": 2,
    "space-infix-ops": 2,
    "semi": [2, "always"],
    "space-before-function-paren": "off"
  },
  "parserOptions": {
    "ecmaVersion": 8
  }
}